*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
SourceArt/**/*.png
SourceArt/**/*.tga

# Binary Files
EatUpAll_project/Binaries/*
EatUpAll_project/Plugins/*/Binaries/*

# Builds
EatUpAll_project/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!Build/*/
Build/*/**
!Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!Build/**/*.ico

# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
EatUpAll_project/Saved/*

# Compiled source files for the engine to use
EatUpAll_project/Intermediate/*
EatUpAll_project/Plugins/*/Intermediate/*

# Cache files for the editor to use
EatUpAll_project/DerivedDataCache/*

### UnrealEngine Patch ###
# Don't ignore icon and splash images for mobile app
!Build/IOS/Resources/
Build/IOS/Resources/*
!Build/IOS/Resources/Graphics/
Build/IOS/Resources/Graphics/*
!Build/IOS/Resources/Graphics/*.png
!Build/Android/res/
Build/Android/res/*
!Build/Android/res/*/
Build/Android/res/*/*
!Build/Android/res/*/*.png
# Ignore plugins binaries on deep subfolders
Plugins/**/Binaries/*
Plugins/**/Intermediate/*

# My Ignore
EatUpAll_project/Intermediate/*
EatUpAll_project/Saved/*
EatUpAll_project/DerivedDataCache/*

# Do not ignore current project's '.uproject'.
!EatUpAll_project/*.uproject

# Do not ignore source, config and plugins dirs.
!EatUpAll_project/Source/*
!EatUpAll_project/Config/*
!EatUpAll_project/Plugins/*